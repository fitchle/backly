{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "declaration": true,
      "outDir": "./build",
      "strict": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "inlineSourceMap": true,
      "moduleResolution":"node",
    },
    "exclude": [
      "test/"
    ],
    "include": [
      "src"
    ],
    "ts-node": {
      "swc": true
    }
}